{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/catalog.html",
      "destination": "/catalog",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
